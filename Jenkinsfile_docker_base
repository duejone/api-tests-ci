pipeline {
    agent any
    triggers {
        pollSCM('*/1 * * * *')
    }
    stages {
        stage('build-docker-base') {
            steps {
                sh -c "docker build -t duejone0/ubuntu_ruby_base:latest . -f Dockerfile_base"
                build job: "Docker Test Executor"
            }
        }
    }
}